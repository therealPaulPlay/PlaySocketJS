{
    "name": "playsocketjs",
    "version": "2.5.2",
    "description": "WebSocket wrapper for creating simple multiplayer systems with ease.",
    "type": "module",
    "module": "dist/playsocket-client.js",
    "exports": {
        ".": {
            "import": "./dist/client.js",
            "types": "./dist/client/client.d.ts"
        },
        "./server": {
            "import": "./dist/server.mjs",
            "types": "./dist/server/server.d.ts"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "start": "node example/example-server.js",
        "build": "rollup -c"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/therealPaulPlay/PlaySocketJS.git"
    },
    "keywords": [
        "WebSocket",
        "data channel",
        "Socket",
        "PlayPeerJS",
        "multiplayer",
        "game"
    ],
    "author": "PaulPlay",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/therealPaulPlay/PlaySocketJS/issues"
    },
    "homepage": "https://github.com/therealPaulPlay/PlaySocketJS#readme",
    "devDependencies": {
        "@rollup/plugin-node-resolve": "^16.0.1",
        "@rollup/plugin-typescript": "^12.1.4",
        "@types/node": "^24.3.0",
        "@types/ws": "^8.18.1",
        "rollup": "^4.50.0",
        "typescript": "^5.9.2"
    },
    "peerDependencies": {
        "ws": "^8.18.1",
        "tslib": "^2.8.1"
    },
    "dependencies": {
        "@msgpack/msgpack": "^3.1.2"
    }
}
